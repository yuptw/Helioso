local function getNearPlayer(maxDistance) if player and player.Character then local playerLocation = player.Character.HumanoidRootPart.Position; for i, v in pairs(game.Players:GetChildren()) do if v.Character and v.Character.Health ~= 0 then local location = v.Character.HumanoidRootPart.Position; if (location - playerLocation).Magnitude <= maxDistance then if v == player then  else plrnum += 1 table.insert(plrlist, v.Character) end end end end end end
